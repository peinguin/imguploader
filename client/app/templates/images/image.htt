
<% if(isOwner){ %>
	<input type="text" value="<%= caption %>" class="title" />
<% }else{ %>
	<span class="title"><%= caption %></span>
<% } %>

<% if(isOwner){ %>
	<span class="remove">X</span>
<% } %>
<img src="<%= url %>" class="img-thumbnail" />
<div class="tags">
	<div class="exists"></div>
	<% if(isOwner){ %>
		<div class="tag new_tag">
			<input type="text" class="tag" /><button class="btn btn-success">+</button>
		</div>
	<% } %>
</div>
